@{
    ViewBag.Title = "403 - Forbidden";
}

@section PageStyles {
    @Styles.Render("~/Content/themes/Error")
}

<div class="error-wrapper" data-ix="fade-in-heading">

    <div class="forbidden-header-wrapper">
        <div class="forbidden-header">
            <div class="forbidden-content">
                <div class="icon error-icon icon-blocked" data-ix="drop-from-top-load"></div>
                <h1 class="error-title" data-ix="fade-in-heading">Nope. <span class="error-name">Forbidden.</span></h1>
                <hr class="title-separator" data-ix="hard-scale-on-scroll-delay-500" />
                <h1 class="error-subtitle" data-ix="hard-scale-on-scroll-delay-1500">You're not allowed to get there.</h1>
            </div>
        </div>
    </div>

    <div class="video-wrapper breakn-a-sweat" data-ix="width-adjust">
        <iframe width="960" height="520" src="https://www.youtube.com/embed/BUULBlDcju4?autoplay=1" frameborder="0" allowfullscreen></iframe>
    </div>

</div>

@section scripts {
    @Scripts.Render("~/bundles/webflow")
}


